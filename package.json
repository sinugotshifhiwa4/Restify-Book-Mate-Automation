{
  "dependencies": {
    "argon2": "^0.44.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "moment-timezone": "^0.6.0",
    "playwright": "^1.55.0",
    "winston": "^3.17.0"
  },
  "name": "restify-book-mate-automation",
  "version": "1.0.0",
  "description": "API test automation framework built with Playwright and TypeScript, designed to validate the functionality of the Restful Booker API",
  "main": "index.js",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "cspell": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "ortoni-report": "^3.0.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "scripts": {
    "type:check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "validate": "npm run type:check && npm run lint",
    "pretest:encryption": "npm run validate",
    "pretest:api": "npm run validate",
    "pretest:ui": "npm run validate",
    "pretest:db": "npm run validate",
    "pretest:failed": "npm run validate",
    "pretest:all": "npm run validate",
    "test:encryption": "cross-env npx playwright test tests/encryption",
    "test:api": "npx playwright test tests/api",
    "test:ui": "npx playwright test tests/ui",
    "test:db": "cross-env npx playwright test tests/db",
    "test:failed": "cross-env npx playwright test --last-failed",
    "test:all": "npx playwright test tests",
    "ui": "npx playwright test --ui",
    "record": "npx playwright codegen",
    "report": "npx playwright show-report",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "playwright",
    "typescript",
    "e2e",
    "api",
    "unit",
    "sanity",
    "regression",
    "report",
    "winston",
    "dotenv",
    "eslint",
    "prettier",
    "argon2"
  ],
  "author": "Tshifhiwa Sinugo",
  "license": "ISC",
  "type": "commonjs"
}
